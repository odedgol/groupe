{
  "name": "groupe",
  "version": "0.1.0",
  "private": true,
  "proxy": "http://localhost:3000/",
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "scripts": {
    "build:dev": "webpack --env.env=development",
    "build:dev:watch": "yarn build:dev --watch --hot",
    "build:dev:analyze": "yarn build:dev --env.addon=bundleanalyze --env.addon=bundlevisualizer",
    "build": "webpack -p --env.env=production",
    "build:watch": "yarn build --watch",
    "build:analyze": "yarn build --env.addon=bundleanalyze --env.addon=bundlevisualizer",
    "lint": "prettier --ignore-path .eslintignore --check \"**/*.{js,jsx,json,css,sass,scss,less,html,md}\" && eslint ",
    "lint:fix": "prettier --ignore-path .eslintignore --write \"**/*.{js,jsx,json,css,sass,scss,less,html,md}\" && eslint  --fix",
    "serve:dev": "webpack-dev-server --mode development --open --env.env=development",
    "serve:dev:dashboard": "webpack-dashboard webpack-dev-server -- --mode development --env.addon=dashboard",
    "start": "yarn serve:dev",
    "serve": "yarn build && serve -s -C build",
    "test": "jest --runInBand --detectOpenHandles --passWithNoTests",
    "test:watch": "jest -u --runInBand --verbose --watch --detectOpenHandles --passWithNoTests",
    "test:coverage": "jest -u --coverage --verbose --runInBand --detectOpenHandles --passWithNoTests"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "prettier --write",
      "yarn lint:fix"
    ],
    "*.{json,css,sass,scss,less,html,md}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@ebay/browserslist-config": "^1",
    "@ebay/skin": "^12",
    "antd": "^4.16.11",
    "prop-types": "^15.7.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^5.2.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.14.8",
    "@babel/core": "^7.15.0",
    "@babel/plugin-proposal-class-properties": "^7.14.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.14.7",
    "@babel/preset-env": "^7.15.0",
    "@babel/preset-react": "^7.14.5",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^27.0.6",
    "babel-loader": "^8.2.2",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-webpack-alias": "^2.1.2",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "clean-webpack-plugin": "^4.0.0-alpha.0",
    "copy-webpack-plugin": "^9.0.1",
    "css-loader": "^6.2.0",
    "dotenv-webpack": "^7.0.3",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.6",
    "enzyme-to-json": "^3.6.2",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-webpack": "^0.13.1",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-jest": "^24.4.0",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.24.0",
    "file-loader": "^6.2.0",
    "html-loader": "^2.1.2",
    "html-webpack-plugin": "^5.3.2",
    "husky": "^7.0.1",
    "inject-html-webpack-plugin": "^1.0.1",
    "jest": "^27.0.6",
    "jest-transform-stub": "^2.0.0",
    "less": "^4.1.1",
    "less-loader": "^10.0.1",
    "lint-staged": "^11.1.2",
    "mini-css-extract-plugin": "^2.2.0",
    "node-sass": "^6.0.1",
    "path": "^0.12.7",
    "prettier": "^2.3.2",
    "react-hot-loader": "^4.13.0",
    "sass-loader": "^12.1.0",
    "serve": "^12.0.0",
    "sinon": "^11.1.2",
    "style-loader": "^3.2.1",
    "terser-webpack-plugin": "^5.1.4",
    "url-loader": "1.1.2",
    "webpack": "^5.48.0",
    "webpack-async-chunk-names-plugin": "^0.1.1",
    "webpack-bundle-analyzer": "^4.4.2",
    "webpack-cli": "^3.3.0",
    "webpack-dashboard": "^3.3.5",
    "webpack-dev-server": "^3.11.2",
    "webpack-hot-middleware": "^2.25.0",
    "webpack-manifest-plugin": "^4.0.2",
    "webpack-merge": "^5.8.0",
    "webpack-visualizer-plugin": "^0.1.11"
  }
}
